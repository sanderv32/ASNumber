{
  "name": "ASnumber",
  "version": "2.5",
  "description": "Fetches AS Number routing information on every visited Webserver",
  "background": {
    "scripts": ["defaults.js","cacheClass.js","backGround.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "update_url": "https://raw.github.com/sanderv32/ASNumber/master/bin/ASNumber-updates.xml",
  "homepage_url": "https://github.com/sanderv32/ASNumber",
  "manifest_version": 2,
  "icons": { "128": "logo.png" },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'unsafe-eval'; connect-src *; object-src 'unsafe-eval'",
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
